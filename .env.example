# Local dev uses Vite proxy: /erp -> http://localhost:1010
VITE_ERP_BASE_URL=
VITE_ERP_API_PATH=/erp/api
VITE_ERP_TIMEOUT=15000

# WARNING: VITE_* values are bundled to browser code.
# Use these only for local/dev testing.
# For production, route through your backend/BFF and keep secrets on server side.
VITE_ERP_API_KEY=
VITE_ERP_API_SECRET=
